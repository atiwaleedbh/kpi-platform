{
  "name": "kpi-platform",
  "version": "1.0.0",
  "description": "A comprehensive KPI monitoring and tracking system",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "server": "nodemon server/index.js",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "test": "jest",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["kpi", "monitoring", "tracking", "analytics", "dashboard"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "body-parser": "^1.20.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "date-fns": "^3.0.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0"
  }
}
